<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#161622"
    tools:context=".activity_tarjeta">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mv_menu_separator"
        tools:layout_editor_absoluteY="380dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mv_menu_separator"
        tools:layout_editor_absoluteY="126dp" />

    <TextView
        android:id="@+id/iniciarSesionText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="88dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Tu Tarjeta"
        android:textColor="#FFFFFF"
        android:textSize="23sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnTransfiya2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="72dp"
        android:backgroundTint="#2C2C2C"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:padding="8dp"
        android:text="Promos"
        android:textColor="@android:color/white"
        app:cornerRadius="12dp"
        app:icon="@drawable/tarjeta"
        app:iconGravity="textStart"
        app:iconPadding="8dp"
        app:iconTint="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="61dp"
        android:layout_height="63dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/fondo_grisoscuro_icono"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/tarjetav2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="74dp"
        android:layout_height="53dp"
        android:text="Congela tu tarjeta"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="507dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="61dp"
        android:layout_height="63dp"
        android:layout_marginEnd="168dp"

        android:layout_marginBottom="232dp"
        android:adjustViewBounds="true"
        android:background="@drawable/fondo_grisoscuro_icono"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton3"
        app:srcCompat="@drawable/factura" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Movimientos"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="507dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="36dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="centerInside"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/question_solid_full"
        tools:layout_editor_absoluteX="311dp" />

    <ImageButton
        android:id="@+id/botonVolver2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lg_boton_volver" />


</androidx.constraintlayout.widget.ConstraintLayout>