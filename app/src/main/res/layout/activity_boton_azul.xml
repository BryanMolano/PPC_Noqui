<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF">


    <ImageButton
        android:id="@+id/btnFondo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/screen_background_dark_transparent" />

    <ImageButton
        android:id="@+id/btnAzul"
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="700dp"
        android:layout_marginEnd="12dp"
        android:adjustViewBounds="true"
        android:background="@drawable/fondo_azul_boton_navbar"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/outline_attach_money_24" />

    <ImageButton
        android:id="@+id/btnEnvia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="12dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/btnQR"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ba_boton_envia" />

    <ImageButton
        android:id="@+id/btnPide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="12dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/btnEnvia"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ba_boton_pide" />

    <ImageButton
        android:id="@+id/btnQR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="12dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/btnRecarga"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ba_boton_qr" />

    <ImageButton
        android:id="@+id/btnRecarga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="16dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/btnAzul"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ba_boton_recarga" />

    <ImageButton
        android:id="@+id/btnSaca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="12dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/btnPide"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ba_boton_saca" />


</androidx.constraintlayout.widget.ConstraintLayout>